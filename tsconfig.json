{
    "compilerOptions": {
        "target": "es6",
        "outDir": "./dist",
        "allowJs": true,
        "checkJs": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "resolveJsonModule": true,
        "pretty": true,
        "esModuleInterop": true,
        "sourceMap": true

    },
    "include": [
        "config.js",
